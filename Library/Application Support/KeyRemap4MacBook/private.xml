<?xml version="1.0"?>
<root>
    <vkopenurldef>
        <name>KeyCode::VK_OPEN_URL_APP_Finder</name>
        <url type="file">/System/Library/CoreServices/Finder.app</url>
    </vkopenurldef>
    <item>
        <name>OPTION_R + N opens Finder</name>
        <identifier>private.right_option_n</identifier>
        <autogen>
            __KeyToKey__
            KeyCode::N, ModifierFlag::OPTION_R,
            KeyCode::VK_OPEN_URL_APP_Finder
        </autogen>
    </item>
    <item>
        <name>OPTION_R + F opens Finder</name>
        <identifier>private.right_option_f</identifier>
        <autogen>
            __KeyToKey__
            KeyCode::F, ModifierFlag::OPTION_R,
            KeyCode::VK_OPEN_URL_APP_Finder
        </autogen>
    </item>
